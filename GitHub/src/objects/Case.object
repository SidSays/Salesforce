    <recordTypes>
        <fullName>Provide_Feedback</fullName>
        <active>true</active>
        <businessProcess>Xfinity Communities Portal</businessProcess>
        <description>This record type is used in XCP for Provide Feedback type case</description>
        <label>Provide Feedback</label>        
    </recordTypes>	
	<validationRules>
        <fullName>Event_Date_cannot_be_in_the_past</fullName>
        <active>true</active>
        <description>The event date when submitting a request for event support on Communities cannot be a date in the past</description>
        <errorConditionFormula>Event_Date__c  &lt;  TODAY() &amp;&amp; (RecordType.Name = &apos;Request Event Support&apos;)</errorConditionFormula>
        <errorDisplayField>Event_Date__c</errorDisplayField>
        <errorMessage>Event date cannot be for a date in the past.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Move_In_Date_cannot_be_in_past</fullName>
        <active>true</active>
        <description>Validation that the date cannot be in the past</description>
        <errorConditionFormula>Move_in_Date__c &lt; TODAY() &amp;&amp; (RecordType.Name = &apos;Request Support for Multiple Move Ins&apos;)</errorConditionFormula>
        <errorDisplayField>Move_in_Date__c</errorDisplayField>
        <errorMessage>Move In date cannot be for a date in the past.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_of_residents_should_be_less_than_1000</fullName>
        <active>true</active>
        <description>Validation Rule for Number of Residents on Request Event Support page</description>
        <errorConditionFormula>AND((RecordType.Name = &apos;Request Event Support&apos;),
OR(Number_of_Residents__c &lt;= 0, Number_of_Residents__c &gt;= 1000))</errorConditionFormula>
        <errorDisplayField>Number_of_Residents__c</errorDisplayField>
        <errorMessage>Number of Residents must be between 1 and 999</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_of_units_should_be_less_than_100</fullName>
        <active>true</active>
        <description>Validation Rule for Number of Units</description>
        <errorConditionFormula>AND((RecordType.Name = &apos;Request Support for Multiple Move Ins&apos;),
OR(Number_of_Units__c &lt;= 0, Number_of_Units__c &gt;= 100))</errorConditionFormula>
        <errorDisplayField>Number_of_Units__c</errorDisplayField>
        <errorMessage>This field must be greater than 0 and less than 100.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Visit_Name_cannot_be_in_past</fullName>
        <active>true</active>
        <description>This validation is used to restrict user from entering the date of past on Community Portal for Request CAR a Visit page.</description>
        <errorConditionFormula>Visit_Date__c &lt;  TODAY() &amp;&amp; (RecordType.Name = &apos;Request a Community Account Representative Visit&apos;)</errorConditionFormula>
        <errorDisplayField>Visit_Date__c</errorDisplayField>
        <errorMessage>Visit date cannot be for a date in the past.</errorMessage>
    </validationRules>
	<validationRules>
        <fullName>Region_is_required</fullName>
        <active>true</active>
        <description>Region field should be mandatory for all profiles except RSA</description>
        <errorConditionFormula>AND( 
	RecordType.Name != &quot;Wave Only User&quot;, 
	ISPICKVAL(Region__c , &quot;&quot;), 
	OR ( 
		AND( 
			OR( 
				$Profile.Name = &quot;Affiliate Claims User&quot;, 
				$Profile.Name = &quot;Claims User&quot;, 
				$Profile.Name = &quot;Claims Manager&quot; 
			), 
			NOT( 
				OR( 
					RecordType.Name != &quot;Closed Claim&quot;, 
					RecordType.Name != &quot;Open Accepted Claim&quot;, 
					RecordType.Name != &quot;Open Assigned Claim&quot;

				) 
			) 
		), 
		AND( 
			$Profile.Name = &quot;Affiliate Escalation User&quot;, 
			NOT( 
				OR( 
					RecordType.Name != &quot;Post-Order Escalation&quot;, 
					RecordType.Name != &quot;Pre-Order Escalation&quot; 
				) 
			) 
		), 
		AND( 
			$Profile.Name = &quot;Gold Partner User&quot;, 
			RecordType.Name = &quot;Master&quot; 
		), 
		AND( 
			$Profile.Name = &quot;XC MCAT Only&quot;, 
			RecordType.Name != &quot;SalesSpace MCAT&quot; 
		), 
		AND( 
			$Profile.Name = &quot;Regional System Admin (RSA)&quot;, 
			NOT( 
				OR( 
					RecordType.Name != &quot;Closed Claim&quot;, 
					RecordType.Name != &quot;Open Accepted Claim&quot;, 
					RecordType.Name != &quot;Open Assigned Claim&quot;, 
					RecordType.Name != &quot;Defect Management&quot; 

				) 
			) 
		), 
		AND( 
			$Profile.Name = &quot;Regional System Admin (RSA)&quot;, 
			OR( 
				RecordType.Name = &quot;SalesSpace Chatter&quot; , 
				RecordType.Name = &quot;SalesSpace Coach Huddle/Forum&quot;, 
				RecordType.Name = &quot;SalesSpace Property Lab&quot;, 
				RecordType.Name = &quot;SR/Ticket Management&quot;, 
				RecordType.Name = &quot;SalesSpace NSO&quot; 

			) 
		), 		
		AND( 
			$Profile.Name != &quot;Regional System Admin (RSA)&quot;, 
			$Profile.Name != &quot;Affiliate Claims User&quot;, 
			$Profile.Name != &quot;Affiliate Escalation User&quot;, 
			$Profile.Name != &quot;Gold Partner User&quot;,
			$Profile.Name != &quot;Claims User&quot;,
			$Profile.Name != &quot;Claims Manager&quot;,
			$Profile.Name != &quot;XC MCAT Only&quot;, 
			$Profile.Name != &quot;XC Property Professional User&quot;,
			AND( 
				OR(
					$Profile.Name = &quot;XC Property Professional User&quot;, 
					$Profile.Name = &quot;System Administrator&quot;
				),
				NOT( 
					OR( 
						RecordType.Name != &quot;XCP Multiple Move In Request&quot;, 
						RecordType.Name != &quot;XCP Property Service Issue&quot;, 
						RecordType.Name != &quot;XCP Request CAR for Visit&quot;, 
						RecordType.Name != &quot;XCP Request Event Support&quot;, 
						RecordType.Name != &quot;Request XFINITY Materials for Residents&quot;,
						RecordType.Name != &quot;Provide Feedback&quot;
					) 
				) 
			)			
		)
		
	) 
)</errorConditionFormula>
        <errorMessage>Region is required.</errorMessage>
    </validationRules>
</CustomObject>
