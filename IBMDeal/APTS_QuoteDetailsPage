                    <apex:stylesheet value="{!URLFOR($Resource.QDP1, 'APTS_QDP.css')}"/>
                    <script>
                        function showPopup(event) {
                            var role = event.currentTarget.parentNode.textContent;
                            role = role.replace(/\s+/g,' ').trim();
                            var roleField = document.getElementById('{!$Component.tstpopupContainer:tstpopupSection:teamRole}');
                            document.getElementById('{!$Component.tstpopup}').style.display = "block";
                            var options = roleField.length;
                            for(i=0;i<options;i++){
                                if(role == roleField.options[i].innerHTML){
                                  roleField.selectedIndex = i;
                                }
                            }
                        }
                        function hidePopup(){
                            document.getElementById('{!$Component.tstpopup}').style.display = "none";
                            return false;     
                        }
                    </script>
